    "webpack-cli": "~4.10.0",
    "ava": "~4.3.0",
    "c8": "~7.11.3",
    "codecov": "~3.8.3",
    "eslint": "~8.23.0",
    "eslint-plugin-ava": "~13.2.0",
    "standard": "~17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rooseveltframework/page-express-mapper.git"
  },
  "keywords": [
    "page.js",
    "express",
    "express-mapper",
    "page.js-express-mapper",
    "pageExpressMapper",
    "page"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "plugins": [
      "ava"
    ],
    "rules": {
      "ava/no-only-test": "error"
    }
  },
  "scripts": {
    "build": "webpack",
    "sample-app": "webpack && http-server -o sampleApp/sampleApp.html",
    "codecov": "codecov",
    "coverage": "c8 --reporter=text --reporter=lcov ava",
    "lint": "standard && eslint test/test.js",
    "test": "ava"
  },
  "funding": {
    "type": "individual",
